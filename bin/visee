#!/bin/sh

this=$0
protocol=$1
command=$2

source $protocol

if [[ $# -eq 1 ]]; then
	compgen -A function | sed \$d

	exit 0
fi

if [[ $# -gt 1 ]]; then
	if $2 2> /dev/null; then
	echo ''
	else
		FUNCTION=`$this $protocol | grep -i $command`
		if [[ $FUNCTION = "" ]]; then
			echo command $command  not found
		else
			echo ' '
			echo $FUNCTION 
			echo ' '
			declare -f $FUNCTION | \sed \$d | tail -n +3
		fi
	fi


fi


